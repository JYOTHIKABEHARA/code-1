include <stdio.h>

int main()
{
    int a,rem,sum=0,temp=0;
    printf("enter your number");
    scanf("%d",&a);
    temp=a;
    while(a>0)
    { rem=a%10;
       sum=sum+rem;
       a=a/10;
    }
    if(temp%sum==0)
    printf("harshed number");
    else
    printf("not harshad number");

    return 0;
}
